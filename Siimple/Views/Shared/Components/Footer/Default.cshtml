@model IDictionary<string,string>
  <footer id="footer">
    <div class="container">
        @if (Model.Any(s => s.Key == "FooterCopyright"))
        {
              <div class="copyright">
                @Model["FooterCopyright"]
              </div>
        }
        @if (Model.Any(s => s.Key == "FooterCredits"))
        {
                  <div class="credits">
                    <!-- All the links in the footer should remain intact. -->
                    <!-- You can delete the links only if you purchased the pro version. -->
                    <!-- Licensing information: https://bootstrapmade.com/license/ -->
                    <!-- Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/free-bootstrap-landing-page/ -->
                   @Model["FooterCredits"]
                  </div>
        }
    </div>
  </footer><!-- End #footer -->